{
  "enclave": {
    "file": "/app/liblskv.virtual.so",
    "type": "Virtual"
  },
  "network": {
    "node_to_node_interface": {
      "bind_address": "127.0.0.1:8001"
    },
    "rpc_interfaces": {
      "main_interface": {
        "bind_address": "0.0.0.0:8000"
      }
    }
  },
  "command": {
    "type": "Start",
    "service_certificate_file": "/app/certs/service_cert.pem",
    "start": {
      "constitution_files": [
        "/app/validate.js",
        "/app/apply.js",
        "/app/resolve.js",
        "/app/actions.js"
      ],
      "members": [
        {
          "certificate_file": "/app/certs/member0_cert.pem",
          "encryption_public_key_file": "/app/certs/member0_enc_pubk.pem"
        }
      ]
    }
  }
}
